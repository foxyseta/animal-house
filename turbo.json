{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "outputs": [],
      "dependsOn": ["^dev"]
    },
    "api#dev": {
      "dependsOn": []
    },
    "game#dev": {
      "dependsOn": []
    },
    "frontoffice#dev": {
      "dependsOn": []
    },
    "backoffice#dev": {
      "dependsOn": []
    },
    "build": {
      "dependsOn": ["^build", "build:cjs", "build:es"]
    },
    "build:cjs": {
      "dependsOn": ["^build:cjs"]
    },
    "build:es": {
      "dependsOn": ["^build:es"]
    }
  }
}
