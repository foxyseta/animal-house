{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "shared",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:cjs": "tsc -p tsconfig.cjs.json --watch --preserveWatchOutput",
    "dev:es": "tsc -p tsconfig.es.json --watch --preserveWatchOutput",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json"
  },
  "devDependencies": {
    "@types/mongoose-paginate": "^5.0.12",
    "@types/node": "^17.0.25",
    "mongoose": "^6.3.1",
    "typescript": "^4.7.4"
  },
  "exports": {
    "./endpoints": {
      "import": "./es/shared/src/endpoints.js",
      "types": "./es/shared/src/endpoints.d.ts",
      "node": "./cjs/shared/src/endpoints.js"
    },
    "./auth": {
      "import": "./es/shared/src/auth.js",
      "types": "./es/shared/src/auth.d.ts",
      "node": "./cjs/shared/src/auth.js"
    },
    "./fetch": {
      "import": "./es/shared/src/fetch.js",
      "types": "./es/shared/src/fetch.d.ts",
      "node": "./cjs/shared/src/fetch.js"
    },
    "./models/category": {
      "import": "./es/shared/src/models/category.js",
      "types": "./es/shared/src/models/category.d.ts",
      "node": "./cjs/shared/src/models/category.js"
    },
    "./models/game-score": {
      "import": "./es/shared/src/models/game-score.js",
      "types": "./es/shared/src/models/game-score.d.ts",
      "node": "./cjs/shared/src/models/game-score.js"
    },
    "./models/pet": {
      "import": "./es/shared/src/models/pet.js",
      "types": "./es/shared/src/models/pet.d.ts",
      "node": "./cjs/shared/src/models/pet.js"
    },
    "./models/product": {
      "import": "./es/shared/src/models/product.js",
      "types": "./es/shared/src/models/product.d.ts",
      "node": "./cjs/shared/src/models/product.js"
    },
    "./models/store": {
      "import": "./es/shared/src/models/store.js",
      "types": "./es/shared/src/models/store.d.ts",
      "node": "./cjs/shared/src/models/store.js"
    },
    "./models/user": {
      "import": "./es/shared/src/models/user.js",
      "types": "./es/shared/src/models/user.d.ts",
      "node": "./cjs/shared/src/models/user.js"
    }
  }
}
